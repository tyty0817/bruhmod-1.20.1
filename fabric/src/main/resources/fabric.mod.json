{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name}",
	"description": "${description}",
	"authors": [
		"Justin", "Tyler", "Rhys"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/tyty0817/le-mod"
	},
	"license": "${license}",
	"icon": "${icon_path}",
	"environment": "*",
	"entrypoints": {
		"main": [
			"name.bruhmod.ModFabric"
		],
		"client": [
			"name.bruhmod.ModFabricClient"
		],
		"fabric-datagen": [
          "name.bruhmod.datagen.ModFabricDataGenerator"
		],
		"jei_mod_plugin": [
			"name.bruhmod.compat.jei.JEIModPlugin"
		]
	},
	"mixins": [
		"${mod_id}.mixins.json",
		"bruhmod_fabric.mixins.json"
	],
	"accessWidener" : "${mod_id}.accesswidener",
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"minecraft": "${minecraft_version}",
		"java": ">=17",
		"fabric-api": "*",
		"geckolib": ">=4.4"
	}
}
